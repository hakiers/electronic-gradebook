<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.egradebook.frontend.controller.student.StudentPageController"
      alignment="CENTER" spacing="20">
    <Button text="Oceny" fx:id="showGradesButton"  style="-fx-background-color: #00d9ff;-fx-text-fill: black;"/>
    <Button text="Plan lekcji" fx:id="showSchedule" style="-fx-background-color: #00d9ff;-fx-text-fill: black;"/>
    <Button text="Zmień hasło" fx:id="changePasswordButton" style="-fx-background-color: #00d9ff;-fx-text-fill: black;"/>
    <Button text="Wyloguj" fx:id="logoutButton"  style="-fx-background-color: red; -fx-text-fill: white;"/>

</VBox>