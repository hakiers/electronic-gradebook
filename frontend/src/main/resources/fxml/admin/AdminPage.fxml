<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.egradebook.frontend.controller.admin.AdminPageController">
    <Button fx:id="addStudentButton" onAction="#handleAddStudent" prefWidth="85.0" style="-fx-background-color: #00d9ff;-fx-text-fill: black;" text="Dodaj ucznia" />
    <Button fx:id="addTeacherButton" onAction="#handleAddTeacher" style="-fx-background-color: #00d9ff;-fx-text-fill: black;" text="Dodaj nauzcyciela" />
    <Button fx:id="changePasswordButton" onAction="#handleChange" style="-fx-background-color: #00d9ff;-fx-text-fill: black;" text="Zmień hasło" />
    <Button fx:id="logoutButton" onAction="#handleLogout" style="-fx-background-color: red; -fx-text-fill: white;" text="Wyloguj" />

</VBox>
