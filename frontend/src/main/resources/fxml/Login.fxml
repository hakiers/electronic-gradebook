<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?><?import javafx.scene.layout.*?>
<VBox spacing="10" fx:controller="com.example.egradebook.controller.LoginController"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <TextField fx:id="usernameField" promptText="Nazwa użytkownika"/>
  <PasswordField fx:id="passwordField" promptText="Hasło"/>
  <Button text="Zaloguj" onAction="#onLogin"/>
</VBox>
